{
    // Workbench
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",

    // misc
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "liveServer.settings.donotShowInfoMsg": true,

    // Explorer
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "vsicons.dontShowNewVersionMessage": true,

    // Terminal
    "terminal.integrated.automationProfile.windows": null,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "autoDocstring.docstringFormat": "numpy",
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path.windows": "C:\\Users\\Dsaad\\.vs-kubernetes\\tools\\helm\\windows-amd64\\helm.exe"
    },

    // Azure
    "azureResourceGroups.groupBy": "resourceGroup",
    "azureResourceManagerTools.parameterFiles": {},

    // Git
    "git.autofetch": true,
    "git.confirmSync": false,

    // Editor
    "editor.accessibilitySupport": "off",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.inlayHints.padding": true,
    "editor.fontFamily": "Consolas, 'Courier New', 'CaskaydiaCove Nerd Font'",
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": false,
    "editor.minimap.enabled": false,

    // Python
    "files.trimTrailingWhitespace": true,
    "testing.automaticallyOpenPeekView": "never",
    "pythonIndent.trimLinesWithOnlyWhitespace": true,
    "python.defaultInterpreterPath": "C:\\Users\\Dsaad\\anaconda3\\envs\\duckdb\\python.exe",
    "python.analysis.inlayHints.variableTypes": true,
    "python.autoComplete.extraPaths": [],
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.extraPaths": [],
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.experiments.optInto": ["pythonREPLSmartSend"],
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordBasedSuggestions": false,
        "editor.formatOnType": true
    },

    // ERROR LENS
    "errorLens.messageEnabled": false,
    "errorLens.editorHoverPartsEnabled": {
        "messageEnabled": true
    },

    // Notebook
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.askForKernelRestart": false,
    "notebook.output.textLineLimit": 100,
    "notebook.lineNumbers": "on",

    // RUST
    "rust-analyzer.checkOnSave": true,
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
    "rust-analyzer.inlayHints.bindingModeHints.enable": true,
    "rust-analyzer.semanticHighlighting.punctuation.separate.macro.bang": true,
    "rust-analyzer.lens.references.trait.enable": true,
    "rust-analyzer.lens.references.method.enable": true,
    "rust-analyzer.lens.references.enumVariant.enable": true,
    "rust-analyzer.lens.references.adt.enable": true,
    "rust-analyzer.interpret.tests": true,
    "rust-analyzer.hover.actions.references.enable": true,
    "rust-analyzer.debug.engineSettings": {
        "lldb": {
            "terminal": "external"
        }
    },

    // Remote
    "remote.SSH.configFile": "C:\\Users\\Dsaad\\.ssh\\config",
    "remote.SSH.remotePlatform": {
        "192.168.178.38": "linux"
    },

    // TODO Tree
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.buttons.export": true,
    "todo-tree.tree.buttons.scanMode": true,
    "todo-tree.tree.filterCaseSensitive": true,
    "todo-tree.tree.buttons.groupBySubTag": true,
    "todo-tree.highlights.useColourScheme": true,
    "todo-tree.highlights.customHighlight": {
        "FIX": {
            "icon": "tools",
            "iconColour": "#FF2D00",
            "gutterIcon": true
        },
        "BUG": {
            "icon": "bug",
            "iconColour": "#FF2D00",
            "gutterIcon": true
        },
        "[ ]": {
            "icon": "issue-draft",
            "iconColour": "#c345e9",
            "gutterIcon": true
        },
        "[x]": {
            "icon": "issue-closed",
            "iconColour": "#521d63",
            "gutterIcon": true
        },
        "LEARN": {
            "icon": "book",
            "iconColour": "#0071B1",
            "gutterIcon": true
        },
        "TODO": {
            "icon": "checkbox",
            "iconColour": "#FF8C00",
            "gutterIcon": true
        },
        "CHECK": {
            "icon": "search",
            "iconColour": "#CA686B",
            "gutterIcon": true
        },
        "IDEA": {
            "icon": "light-bulb",
            "iconColour": "#861657",
            "gutterIcon": true
        },
        "NOTE": {
            "icon": "note",
            "iconColour": "#569cd6",
            "gutterIcon": true
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "FIX",
        "TODO",
        "[ ]",
        "[x]",
        "LEARN",
        "CHECK",
        "IDEA",
        "NOTE"
    ],
    "todo-tree.general.showActivityBarBadge": true,
    "todo-tree.highlights.enabled": false,

    // Better Comments
    "better-comments.highlightPlainText": true,
    "better-comments.tags": [
        {
            "tag": "FIX",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold":true,
            "italic": false
        },
        {
            "tag": "BUG",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": "TODO",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "INFO",
            "color": "#0C7C59",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "LEARN",
            "color": "#0071B1",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "STEP",
            "color": "#92524f",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "CHECK:",
            "color": "#CA686B",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "[ ]",
            "color": "#c345e9",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "[X]",
            "color": "#521d63",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "IDEA",
            "color": "#861657",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "NOTE",
            "color": "#569cd6",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],

    // Codeium
    "codeium.enableConfig": {
        "*": true,
        "markdown": true,
        "rust": false
    },

    // Sourcery
    "sourcery.codeLens": false
}